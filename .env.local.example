# Stripe API keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Stripe testing mode
NEXT_PUBLIC_STRIPE_TEST_MODE=true

# PayPal API keys
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id

# PayPal API URL - use the sandbox URL for testing
PAYPAL_API_URL=https://api-m.sandbox.paypal.com
# For production use: https://api-m.paypal.com

# Site URL for return URLs
NEXT_PUBLIC_SITE_URL=https://projects-higher-agent.vercel.app
